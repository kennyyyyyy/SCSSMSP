<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>  	
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Control.LoginServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>LoginOutServlet</servlet-name>
        <servlet-class>Control.LoginOutServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ImportStudentServlet</servlet-name>
        <servlet-class>Control.AdminServlet.ImportStudentServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>CheckSnoServlet</servlet-name>
        <servlet-class>Control.AdminServlet.CheckSnoServlet</servlet-class>
    </servlet>



    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>LoginOutServlet</servlet-name>
        <url-pattern>/loginOut</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>ImportStudentServlet</servlet-name>
        <url-pattern>/import</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>CheckSnoServlet</servlet-name>
        <url-pattern>/checkSno</url-pattern>
    </servlet-mapping>
</web-app>